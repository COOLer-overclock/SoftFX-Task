@{
    ViewBag.Title = "Home Page";
}

<script>
    $(document).ready(function () {
        $.ajax({
            method: "GET",
            url: "https://ttdemowebapi.soft-fx.com:8443/api/v1/public/symbol",
            dataType: "json",
            success: function (data) {
                console.log('Received data from external source!');
                var symbolsArray = new Array();
                $(data).each(function (index, value) {
                    symbolsArray[symbolsArray.length] = value.Symbol;
                })
            }
        })
    })
</script>